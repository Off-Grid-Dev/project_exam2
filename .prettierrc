{
  "tabWidth": 2,
  "useTabs": false,
  "arrowParens": "always",
  "bracketSameLine": false,
  "bracketSpacing": true,
  "htmlWhitespaceSensitivity": "css",
  "jsxSingleQuote": true,
  "printWidth": 80,
  "quoteProps": "consistent",
  "semi": true,
  "trailingComma": "all",
  "singleQuote": true,
  "singleAttributePerLine": false,
  "plugins": ["prettier-plugin-tailwindcss"]
}
